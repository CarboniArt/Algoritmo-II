//ex 1
#include <iostream>
#define N 5
using namespace std;


int busca_sequencial(int *v, int x);

int main(){
    int v[N] = {1, 3, 5, 7, 9,};
    int indice, x;
    x = 8;
    indice = busca_sequencial (v, x);
    cout << "O índice j tal que v[j] < x ≤ v[j+1] e " << indice;
    return 0;
}

int busca_sequencial(int *v, int x){
    int j = 0; 
    while (j < N && v[j] < x) {
        j++;
    }
    return j;
}

